d);\n    }\n\n    Engine = new engine();\n    onPlanetLoaded = (planet) => {\n        this.setState({planet, loading: false, error: false}\n        );\n    };\n    onError = (err) => {\n        this.setState({error: true, loading: false})\n    }\n\n    updatePlanet = () => {\n        const id = this.randomInteger(0, 10);\n        this.Engine\n            .getPlanet(id)\n            .then(this.onPlanetLoaded)\n            .catch(this.onError);\n    }\n\n\n    render() {\n\n        let {planet, loading, error} = this.state;\n        let hasData = !(loading || error);\n        let spinner = (loading) ? <Spinner/> : null;\n        let data = (hasData) ? <PageView planet={planet}/> : null;\n        let errorMesage = (error) ? <DisplayError/> : null;\n        return (\n            <div className=\"random-planet jumbotron rounded\">\n                {spinner}\n                {data}\n                {errorMesage}\n            </div>\n        );\n    }\n}\n","D:\\js\\4.git\\stardb\\src\\components\\item-list\\index.js",[],"D:\\js\\4.git\\stardb\\src\\components\\item-list\\item-list.js",[],"D:\\js\\4.git\\stardb\\src\\engine\\engine.js",[],"D:\\js\\4.git\\stardb\\src\\components\\spinner\\index.js",[],"D:\\js\\4.git\\stardb\\src\\components\\spinner\\spinner.js",[],"D:\\js\\4.git\\stardb\\src\\components\\onError\\index.js",[],"D:\\js\\4.git\\stardb\\src\\components\\onError\\onError.js",[],"D:\\js\\4.git\\stardb\\src\\components\\random-planet\\PageView.js",[],"D:\\js\\4.git\\stardb\\src\\components\\ErrorButton\\index.js",[],"D:\\js\\4.git\\stardb\\src\\components\\ErrorButton\\ErrorButton.js",[],"D:\\js\\4.git\\stardb\\src\\components\\person-list\\index.js",[],["136","137"],"D:\\js\\4.git\\stardb\\src\\components\\person-list\\person-list.js",[],"D:\\js\\4.git\\stardb\\src\\components\\item-details\\index.js",[],"D:\\js\\4.git\\stardb\\src\\components\\item-details\\item-details.js",["138"],"D:\\js\\4.git\\stardb\\src\\components\\app\\app.js",[],"D:\\js\\4.git\\stardb\